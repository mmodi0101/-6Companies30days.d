void matchPairs(char nuts[], char bolts[], int n) {
	    // code here
	    map<char, int> mp;
	    char arr[9] = {'!', '#', '$', '%','&', '*', '@', '^', '~'};
	    int i,j=0;
	    for(i=0;i<n;i++)
	    {
	        mp[nuts[i]] = 1;
	    }
	    for(i=0;i<9;i++)
	    {
	        if(mp[arr[i]] == 1)
	        {
	            nuts[j] = arr[i];
	            bolts[j] = arr[i];
	            j++;
	        }
	    }
	}

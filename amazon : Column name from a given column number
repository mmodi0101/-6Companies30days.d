string colName (long long int n)
    {
        // your code here  1 a  2 b  3 c 
        
        string ans="";
        
        while(n)
        {
            char ch='A' + ((n-1)%26);
            ans=ch+ans;
            n=(n-1)/26;
        }
        
        
        return ans;
        //28%26 = 2 = b
    }
